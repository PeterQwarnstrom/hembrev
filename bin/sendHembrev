#!/usr/bin/env node

var hembrev = require('../scrapers/hembrev');
var mailsender = require('../mailsender');

hembrev.scrape(function (err, data) {
    if (err) throw err;

    mailsender.send(data, function (err, data) {
        if (err) throw err;
        console.log(JSON.stringify(data, null, 4));
    });
});
process.exit();